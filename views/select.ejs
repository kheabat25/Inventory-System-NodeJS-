<h1>List of Items</h1>

<table>
  <tr>
    <td>id</td>
    <td>name</td>
    <td>qty</td>
    <td>amount</td>
  </tr>
  <% items.forEach(function(item) { %>
  <tr>
    <td><%= item.id %></td>
    <td><%= item.name %></td>
    <td><%= item.qty %></td>
    <td><%= item.amount %></td>
  </tr>
  <% }); %>
</table>
